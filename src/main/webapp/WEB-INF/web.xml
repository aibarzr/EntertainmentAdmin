<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CreateConsole</servlet-name>
  	<display-name>CreateConsole</display-name>
  	<description></description>
  	<servlet-class>servlets.CreateConsole</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteConsole</servlet-name>
  	<display-name>DeleteConsole</display-name>
  	<description></description>
  	<servlet-class>servlets.DeleteConsole</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateConsole</servlet-name>
  	<url-pattern>/createConsole</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteConsole</servlet-name>
  	<url-pattern>/DeleteConsole</url-pattern>
  </servlet-mapping>
</web-app>
